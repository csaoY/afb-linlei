
page{
    background: #f5f5f5;
}
.header{
    background: #fff;padding:15px 30rpx;display:flex;
}
.header>.img-wrapper{
width:40px;height:40px;margin-right:10px;
}
.header>.img-wrapper>image{
    width:40px;height:40px;
}
.header>.info{
    font-size:14px;color:#666;flex:2;
}
.header>.info>view{
line-height:20px;
}
.header>.info>view>text{
    color:#333;
}
.header>.copy-wrapper{
    flex:1;
}
.header>.copy-wrapper>.copy{
    border:1px solid #00C0A3;color:#00C0A3;
    line-height:24px;width:60px;text-align:center;
    border-radius: 12px;float: right;margin-top:16px;
}
.detail-wrapper{
    padding:15px 30rpx;background: #fff;margin-top:10px;
}
.detail{
    display:flex;line-height:1.5;align-items:center
}

.detail>.time{
    width:100px;text-align:right;color:#666;;padding:16px 0;
    border-right:1px solid #eee;padding-right:16px;position: relative;
    box-sizing:border-box

}
.detail>.time:after{
   display:block;content:"";position:absolute;width:8px;
height:8px;background-color:#eee;border-radius:6px;
top:50%;right:-4px;margin-top:-4px;
}

.detail-wrapper>.detail:first-child>.time:after{
    background-color:#00C0A3;width:12px;height:12px;bottom:-6px;right:-6px
}


.detail>.process{
    flex:1;padding-left:15px;
    font-size:13px;color:#999;
}
.detail>.process>view{
    display: table-cell;vertical-align: middle;align-items:center;
}
.detail-wrapper>.detail:first-child>.process{
color:#222;font-size:14px
}