page {
 
  background:#fff;
}
.clear::after{
  display:table;
  content:'';
  clear:both;
}
.bar{
  height:10px;background: #f5f5f5;
}
.box-wrapper{
    padding:20px 15px;line-height:2;border-bottom:1px solid #f5f5f5;
}
.box-wrapper>.box-list{
    display:flex;font-size:14px;color:#999;
}
.box-wrapper>.box-list>view:first-child{
flex:1;
}
.box-wrapper>.box-list>view:nth-child(2){
flex:1;text-align:right;
}
.top{
    text-align:center;padding:30px 0 55px 0;
}
.top>image{
     width:120rpx;
}
.top>.fir{
   color:#333;margin-top:15px;font-size:17px;color:#333;
}
.top>.sec{
    margin-top:10px;color:#999;font-size:15px;
}

.top>view>image{
    width:101.5px;height:11.5px;margin-top:10px;
}

.big-bg{
    background-color:rgba(0,0,0,0.5);position: fixed;top:0;bottom:0;right:0;left:0;
}
.big-bg>.cancelOrder{
    width:540rpx;height:300px;background: #fff;position:absolute;top:50%;left:50%;
    margin-left:-270rpx;margin-top:-150px;
     transition: all .3s;
    transform: translate3d(0 100% 0)
}


.alert-box{
    padding-top:26px;background:#fff;text-align:center;position:fixed;top:50%;left:50%;
    width:540rpx;box-sizing: border-box;border-radius:6px;
    transform: translate(-50%,-50%);margin-top:-20px;
}
.alert-box>.title{
    font-size:16px;color:#333;padding-bottom:10px;
}
.alert-box>.content{
    font-size:13px;color:#666;padding:0 60rpx 20px 60rpx;
    overflow-y:auto;line-height:25px;text-align:left;
}
.alert-box>.content>.why{
    font-size:13px;color:#999;text-align:left;padding:5px 0;
}
.alert-box>.content>.list{
    padding:5px 0;display:flex;
}
.alert-box>.content>.list>.one{
  width:20px;height:20px;margin-right:10px;border-radius:50%;border:1px solid #999;box-sizing:border-box;
}
.alert-box>.content>.list>.one.active{
    background-image:url('/img/agreen.png');background-size:20px;border:0;
}
.alert-box>.content>.list>.two{
    font-size:13px;color:#666;text-align:left;flex:1;line-height:20px;
}
.alert-box>.footer-option{
display:flex;border-top:1px solid #eee;
}
.alert-box>.footer-option>.btn{
    height:50px;line-height:50px;flex:1;
}
.alert-box>.footer-option>view:first-child{
    border-right:1px solid #eee;
}
.alert-box>.footer-option>view:last-child{
    color:#0dd7aa
}
.alert-box>.close-alert{
    width:48px;height:48px;position:absolute;bottom:-68px;left:50%;margin-left:-24px;
   
}
.alert-box>.close-alert>image{
     width:48px;height:48px;
    }
.bg{
    position:fixed;top:0;left:0;bottom:0;right:0;background:rgba(0,0,0,0.5);
    
}



.long-btn{
    height:44px;line-height:44px;color:#fff;text-align:center;width:100%;
    background: #0dd7aa;display:inline-block;border-radius:22px;font-size:16px;
}



.good-info{
    padding:20px 30rpx;display:flex;background: #fff;
}
.good-info >.img-wrapper{
    width:80px;height:80px;text-align:center;
}
.good-info>.detail{
    margin-left:10px;flex:1;
}
.good-info>.detail>.title{
    font-size:14px;color:#333;line-height:1.5;
}
.good-info>.detail>.guige{
    font-size:12px;color:#999;line-height:1.5;
}
.good-info>.detail>.rent{
    font-size:14px;color:#333;line-height:1.5;
}
.good-info>.detail>.zhima{
    border:1px solid #1ccfc9;color:#1ccfc9;display:inline-block;padding:2px 4px;font-size:10px;
}
button{
    border:none
}
.top{
padding:30px 0 55px 0;background: #fff;
}
button.btn-def{
    background: #00c0a3;color:#fff;height:44px;line-height:44px;
    width:690rpx;
    margin:30px auto;
    border-radius:22px;
}

