.big-bg{
    background-color:rgba(0,0,0,0.5);
    position: fixed;top:0;bottom:0;right:0;
    left:0;z-index:2;
}
.big-bg>.cancelOrder{
    width:540rpx;height:300px;background: #fff;
    position:absolute;top:50%;left:50%;
    margin-left:-270rpx;margin-top:-150px;
    transition: all .3s;
    transform: translate3d(0 100% 0)
}